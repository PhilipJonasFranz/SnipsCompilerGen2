/*
 * [BASE-PACKAGE]
 * 
 * This package contains the interface definition for an iterable struct.
 * The interface creates data access and exchange points, so data can be
 * read and written.
 */
interface Iterable<T> {

	/* Returns the size of the iterable struct */
	shared int size<T>();
	
	/* Returns the value at the given index. */
	shared T get<T>(int index);
	
	/* Set the value at the given index to the given value. */
	shared void set<T>(int index, T value);

}